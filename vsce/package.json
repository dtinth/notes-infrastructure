{
  "name": "dtinth-notes",
  "displayName": "dtinth-notes",
  "description": "Provides extra functionality for note-taking",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:dtinth-notes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dtinth-notes.refresh",
        "title": "Refresh notes sidebar",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dtinth-notes.refresh",
          "when": "view == dtinth-notes",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "dtinth-notes",
          "name": "Notes"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "dtinth-notes",
        "contents": "Notes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "axios": "^0.21.0"
  }
}
