{
  "ego.power-tools": {
    "commands": {
      "dtinth.journal.linkAssistant": {
        "script": "linkAssistant.js",
        "button": {
          "text": "$(link)"
        }
      },
      "dtinth.journal.openInWeb": {
        "script": "openInWeb.js",
        "button": {
          "text": "$(globe)"
        }
      },
      "dtinth.journal.preview": {
        "script": "preview.js",
        "button": {
          "text": "$(open-preview)"
        }
      },
      "dtinth.journal.search": {
        "script": "search.js",
        "button": {
          "text": "$(search)"
        }
      }
    }
  },
  "vscodeMarkdownNotes.allowPipedWikiLinks": true,
  "markdown.styles": [
    "https://cdn.jsdelivr.net/combine/npm/water.css@2.0/out/dark.min.css,gh/dtinth/dtinth-water@v0.3.0/dtinth-water.min.css"
  ]
}
